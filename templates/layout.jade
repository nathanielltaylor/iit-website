              a(href="http://tononi.psychiatry.wisc.edu/index.php", target="_blank") About
    .container#content
      .wrap
        != typogr(page.html).typogrify()
        block content
    .footer
      .container-fluid
        - var d = new Date();
        .copyright &copy; Copyright #{d.getFullYear()} #{locals.owner}&nbsp;&nbsp;&middot;&nbsp;&nbsp;Updated #{d.getMonth()}/#{d.getDate()}/#{d.getFullYear()}
