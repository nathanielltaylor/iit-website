@import "solarized-colors"

pre, code, pre.prettyprint, code.prettyprint
  background-color base03
  -moz-border-radius 4px
  -webkit-border-radius 4px
  -o-border-radius 4px
  -ms-border-radius 4px
  -khtml-border-radius 4px
  border-radius 4px
  color base0

code, code.prettyprint
  padding .4ex 1.1ex
  margin 0 0.2ex
  font-size 90%

pre, pre.prettyprint
  font-size 95%
  width 100%
  margin 1em 0
  padding .8em 1em
  white-space pre-wrap
  border none

.kwd
  color blue
.pln
  color base0
.pun
  color base1
.typ
  color yellow
.com
  color base01
  font-style italic
.lit
  color cyan
.str
  color cyan
.tag
  color blue
.atn
  color base0
.atv
  color cyan
.opn
  color base1
.clo
  color base1
.dec
  color base1
  font-style italic
pre.prettyprint ol.linenums
  color base01

